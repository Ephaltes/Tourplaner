<Window x:Class="frontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:frontend.ViewModels"
        xmlns:navigation="clr-namespace:frontend.Navigation"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ContentControl Content="{Binding CurrentViewModel }"></ContentControl>
        <Button Content="Switch to TestView" Command="{Binding UpdateCurrentViewModelCommand}" CommandParameter="{x:Static navigation:ViewType.TestView}" Margin="64,244,64,18" />
    </Grid>
</Window>
