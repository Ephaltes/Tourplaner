<Application x:Class="frontend.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:frontend" 
             xmlns:viewmodels="clr-namespace:frontend.ViewModels"
             xmlns:view="clr-namespace:frontend.Views">
    <Application.Resources>
        
        <DataTemplate DataType="{x:Type viewmodels:DefaultViewModel}">
            <view:DefaultView></view:DefaultView>
        </DataTemplate>
        
        <DataTemplate DataType="{x:Type viewmodels:TestViewModel}">
             <view:TestView></view:TestView>
         </DataTemplate>
        
    </Application.Resources>
</Application>
